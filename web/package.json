{
  "name": "web",
  "version": "0.0.0",
  "description": "A React/Flux app generate by RF, powered with JavaScript",
  "main": "build/bundle.js",
  "dependencies": {
    "flux": "^2.0.3",
    "object-assign": "^2.0.0",
    "react": "^0.13.3",
    "react-router": "^0.13.3"
  },
  "devDependencies": {
    "css-loader": "*",
    "file-loader": "*",
    "jest-cli": "*",
    "react-hot-loader": "*",
    "react-tools": "*",
    "style-loader": "*",
    "url-loader": "*",
    "webpack-dev-server": "*",
    "webpack": "*",
    "jsx-loader": "*",
    "less": "*",
    "less-loader": "*",
    "bootstrap": "*",
    "bootstrap-webpack": "*",
    "imports-loader": "*",
    "jquery": "*"
  },
  "scripts": {
    "build": "webpack",
    "dev": "webpack-dev-server --colors --content-base ./build",
    "test": "jest"
  },
  "author": "",
  "license": "MIT",
  "jest": {
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "testFileExtensions": [
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "globals": {
      "SCRIPTS": "<rootDir>/src/scripts"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/node_modules/"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ]
  }
}
